{
  "name": "wbot",
  "version": "0.60.0",
  "description": "A simple whatsapp reply bot using puppeteer.",
  "main": "src/index.js",
  "scripts": {
    "app": "node src/index.js",
    "pack": "pkg .",
    "start": "npm install && nodemon src/index.js"
  },
  "bin": {
    "wbot": "src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasani-arpit/WBOT.git"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "automation"
  ],
  "private": true,
  "pkg": {
    "scripts": "src/**/*.js",
    "targets": [
      "node18-win",
      "node18-linux",
      "node18-macos"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js"
  },
  "author": "Arpit Vasani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vasani-arpit/WBOT/issues"
  },
  "homepage": "https://github.com/vasani-arpit/WBOT#readme",
  "dependencies": {
    "cli-progress": "^3.12.0",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "mel-spintax": "^1.0.4",
    "mime": "^2.4.4",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.1",
    "open": "^10.1.0",
    "openurl": "^1.1.1",
    "ora": "^3.0.0",
    "puppeteer-core": "^13.6.1",
    "qrcode-terminal": "^0.12.0",
    "semver": "^7.6.3",
    "whatsapp-web.js": "^1.25.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@yao-pkg/pkg": "^5.12.1",
    "nodemon": "^3.1.4"
  },
  "directories": {
    "doc": "docs"
  }
}
